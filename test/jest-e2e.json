{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^@core/(.*)$": "<rootDir>/../src/core/$1",
    "^@nest-modules/(.*)$": "<rootDir>/../src/nest-modules/$1",
    "^@database$": "<rootDir>/../src/database",
    "^@database/(.*)$": "<rootDir>/../src/database/$1"
  }
}
